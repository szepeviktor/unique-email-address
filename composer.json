{
    "name": "szepeviktor/unique-email-address",
    "type": "library",
    "description": "",
    "license": "MIT",
    "require": {
        "php": "~7.2 || ~8.0",
        "ext-mbstring": "*",
        "ext-intl": "*",
        "jeremykendall/php-domain-parser": "^6.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "SzepeViktor\\UniqueEmailAddress\\": "src/"
        }
    },
    "prefer-stable": true
}
